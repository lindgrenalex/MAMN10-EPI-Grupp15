    <module
        class = "Constant"
        name = "MovingSpeed"
        outputsize = "1"
        data = "0.5"
    
    />
    
    <connection
        sourcemodule = "MovingSpeed"
        source = "OUTPUT"
        targetmodule = "Dynamixel1"
        target = "MOVING_SPEED"
        
    />
    
    <connection
