<?xml version="1.0"?>

<group title="Dynamixel Example">
    
    
    <module
        class = "Dynamixel"
        name = "Dynamixel1"
        device  = "/dev/cu.usbserial-A4012AK3"
        index_mode = "indirect"
        printf_info ="full"
        use_feedback ="true"
        baud_rate = "1000000"
        angle_unit = "degrees"
        servo_id = "1"
    />
    
    <module
        class = "Dynamixel"
        name = "Dynamixel2"
        device  = "/dev/cu.usbserial-A4012AK3"
        index_mode = "indirect"
        printf_info ="full"
        use_feedback ="true"
        baud_rate = "1000000"
        angle_unit = "degrees"
        servo_id = "2"
    />
    
