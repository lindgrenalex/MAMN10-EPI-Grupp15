    angle_unit = "degrees"
    servo_id = "3"
    />
    
    <module
        class = "Constant"
        name = "TorqueEnable"
        outputsize = "1"
        data = "1"
    />
    
    <connection
        sourcemodule = "TorqueEnable"
        source = "OUTPUT"
        targetmodule = "Dynamixel1"
        target = "TORQUE_ENABLE"
        
    />
    
    
    <connection
        sourcemodule = "TorqueEnable"
        source = "OUTPUT"
        targetmodule = "Dynamixel1"
        target = "TORQUE_ENABLE"
        
    />
    
    
